<% if (!contact ) { %>
<div class="alert alert-danger fw-bold" role="alert">
   Contact tidak di temukan
   <br />
   <a href="/contact" class="alert-link fw-normal">Kembali ke daftar contact</a
   >.
</div>
<% } else { %>
<div class="card" style="width: 18rem">
   <div class="card-body">
      <h5 class="card-title"><%= contact.nama %></h5>
      <h6 class="card-subtitle my-2 text-muted"><%= contact.nohp %></h6>
      <p class="card-text"><%= contact.email %></p>
      <a href="/contact" class="card-link">&laquo; Daftar contact</a>
      <!-- <a href="#" class="card-link">Another link</a> -->
   </div>
</div>
<% } %>
